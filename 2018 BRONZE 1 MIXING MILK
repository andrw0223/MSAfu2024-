#include <iostream>
#include <algorithm>
using namespace std;

int main()
{
	int c1, m1, c2, m2, c3, m3;
	cin >> c1 >> m1;
	cin >> c2 >> m2;
	cin >> c3 >> m3;
	int buckets[3] = { m1, m2, m3 };
	for (int i = 0; i < 32; i++)
	{
		if (buckets[0] + buckets[1] < c2 + 1)
		{
			buckets[1] += buckets[0];
			buckets[0] = 0;
		}
		else
		{
			buckets[0] = buckets[0] + buckets[1] - c2;
			buckets[1] = c2;
		}
		if (buckets[1] + buckets[2] < c3 + 1)
		{
			buckets[2] += buckets[1];
			buckets[1] = 0;
		}
		else
		{
			buckets[1] = buckets[1] + buckets[2] - c3;
			buckets[2] = c3;
		}
		if (buckets[2] + buckets[0] < c1 + 1)
		{
			buckets[0] += buckets[2];
			buckets[2] = 0;
		}
		else
		{
			buckets[2] = buckets[2] + buckets[0] - c1;
			buckets[0] = c1;
		}
	}
	if (buckets[0] + buckets[1] < c2 + 1)
	{
		buckets[1] += buckets[0];
		buckets[0] = 0;
	}
	else
	{
		buckets[0] = buckets[0] + buckets[1] - c2;
		buckets[1] = c2;
	}
	cout << buckets[0] << '\n';
	cout << buckets[1] << '\n';
	cout << buckets[2];

	return 0;
}

